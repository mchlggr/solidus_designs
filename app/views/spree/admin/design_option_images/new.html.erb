<%= render :partial => 'spree/admin/shared/product_tabs', :locals => {:current => 'Personlization'} %>
<%= render :partial => 'spree/shared/error_messages', :locals => {:target => @design_option_image} %>

<%= form_for [:admin, @design_option, @design_option_image], url: admin_design_option_images_path(@design_option), :html => {:multipart => true} do |f| %>
    <fieldset data-hook="new_image">
      <legend align="center"><%= Spree.t(:new_image) %></legend>

      <%= render :partial => 'form', :locals => {:f => f} %>
      <%= render :partial => 'spree/admin/shared/new_resource_links' %>
    </fieldset>
<% end %>
